<?php
// Nhập số nguyên dương N từ dòng lệnh
echo "Nhập một số nguyên dương có 2 chữ số: ";
$N = intval(trim(fgets(STDIN)));

// Kiểm tra xem N có phải là số nguyên dương có 2 chữ số không
if ($N < 10 || $N > 99) {
    echo "Vui lòng nhập một số nguyên dương có 2 chữ số.";
} else {
    // Tính tổng các chữ số của N
    $tong = floor($N / 10) + $N % 10;

    // In ra màn hình tổng các chữ số của N
    echo "Tổng các chữ số của $N là: $tong";
}
?>
